{
  "root": true,
  "ignorePatterns": ["**/*"],
  "plugins": ["@nx"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "extends": ["@nx/eslint-plugin/base"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "extends": ["@nx/eslint-plugin/typescript"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "extends": ["@nx/eslint-plugin/javascript"],
      "rules": {}
    }
  ]
}
